{
  "name": "skycell",
  "version": "1.0.0",
  "description": "Karate is an open-source tool to for API test and performance automation.",
  "main": "index.js",
  "scripts": {
    "karate:run:project": "dotenv cross-var -- bash -c 'mvn clean test -DauthUrl=$authUrl -DverificationUrl=$verificationUrl -DsensorUrl=$sensorUrl -DapiKey=$apiKey'",
    "karate:run:feature": "dotenv cross-var -- bash -c 'mvn clean test \"-Dkarate.options=--tags $TEST_FEATURE \" -DauthUrl=$authUrl -DverificationUrl=$verificationUrl -DsensorUrl=$sensorUrl -DapiKey=$apiKey'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.3.0"
  }
}
